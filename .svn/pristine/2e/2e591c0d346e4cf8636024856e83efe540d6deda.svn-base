package com.kfwy.hkp.cooperate.entity;

import com.gniuu.framework.entity.BaseEntity;

/**
 * 用户合作评价实体类
 */
public class EvaluateEntity extends BaseEntity {

    //被评价者
    protected String toUserCode;
    protected String toUserName;
    protected String toUserImg;
    //评价者
    protected String fromUserCode;
    protected String fromUserName;
    protected String fromUserImg;
    //评价内容
    protected String content;


    public String getToUserName() {
        return toUserName;
    }

    public String getToUserImg() {
        return toUserImg;
    }

    public String getFromUserName() {
        return fromUserName;
    }

    public String getFromUserImg() {
        return fromUserImg;
    }

    public void setToUserName(String toUserName) {
        this.toUserName = toUserName;
    }

    public void setToUserImg(String toUserImg) {
        this.toUserImg = toUserImg;
    }

    public void setFromUserName(String fromUserName) {
        this.fromUserName = fromUserName;
    }

    public void setFromUserImg(String fromUserImg) {
        this.fromUserImg = fromUserImg;
    }

    public void setFromUserCode(String fromUserCode) {
        this.fromUserCode = fromUserCode;
    }

    public void setContent(String content) {
        this.content = content;
    }

    public void setToUserCode(String toUserCode) {
        this.toUserCode = toUserCode;
    }

    public String getToUserCode() {
        return toUserCode;
    }

    public String getFromUserCode() {
        return fromUserCode;
    }

    public String getContent() {
        return content;
    }
}

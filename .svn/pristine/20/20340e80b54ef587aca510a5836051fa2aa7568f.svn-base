package com.kfwy.hkp.crm.customer.entity;

import com.gniuu.framework.entity.BaseEntity;
import com.sun.org.apache.xpath.internal.operations.Bool;

import java.util.Date;


/**
 * 集中获客实体类
 *
 * @author liwensihan
 */
public class FocusCusEntity extends BaseEntity {

    // 集中获客类型 new old
    private String focusType;
    //用户编码
    private String userCode;
    // 用户部门编码
    private String userDeptCode;
    //客户编码
    private String cusCode;
    //处理状态
    private Integer handleStatus;

    private boolean handle;

    public Date getSeeCusTime () {
        return seeCusTime;
    }

    public void setSeeCusTime (Date seeCusTime) {
        this.seeCusTime = seeCusTime;
    }

    private Date seeCusTime;

    private Boolean specialPush;

    public Boolean getSpecialPush () {
        return specialPush;
    }

    public void setSpecialPush (Boolean specialPush) {
        this.specialPush = specialPush;
    }

    public String getFocusType() {
        return focusType;
    }

    public void setFocusType(String focusType) {
        this.focusType = focusType;
    }

    public String getUserCode() {
        return userCode;
    }

    public void setUserCode(String userCode) {
        this.userCode = userCode;
    }

    public String getUserDeptCode() {
        return userDeptCode;
    }

    public void setUserDeptCode(String userDeptCode) {
        this.userDeptCode = userDeptCode;
    }

    public String getCusCode() {
        return cusCode;
    }

    public void setCusCode(String cusCode) {
        this.cusCode = cusCode;
    }

    public Integer getHandleStatus() {
        return handleStatus;
    }

    public void setHandleStatus(Integer handleStatus) {
        this.handleStatus = handleStatus;
    }

    public boolean isHandle() {
        return handle;
    }

    public void setHandle(boolean handle) {
        this.handle = handle;
    }
}

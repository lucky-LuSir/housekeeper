package com.kfwy.hkp.base;

public interface ISystemConfigRedisDao {

    /**
     * 添加配置信息
     *
     * @param sce
     */
    void addConfig(SystemConfigEntity sce);

    /**
     * 获取配置信息
     *
     * @param key
     * @return
     */
    SystemConfigEntity getConfig(String key);

    /**
     * 获取配置值
     *
     * @param key
     * @return
     */
    String getConfigValue(String key);

    /**
     * 删除配置信息
     *
     * @param key
     */
    void deleted(String key);

}

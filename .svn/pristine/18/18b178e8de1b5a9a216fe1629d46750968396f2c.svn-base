    <select id="queryRegionEmployee" parameterType="string" resultMap="BaseResultMap"
            resultType="com.kfwy.hkp.hrm.org.employee.entity.EmployeeEntity">
        select
            e.emp_code,
            e.emp_name,
            e.dept_code
        from t_hkp_hrm_org_employee e
        left join t_hkp_hrm_org_dept d on d.dept_code = e.dept_code
        where d.region_code = #{regionCode} and e.is_deleted = false
    </select>


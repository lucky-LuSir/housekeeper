package com.kfwy.hkp.trade.order.config;

import java.util.List;

/**
 * 员工岗位配置
 */
public class EmpPostConfig {

    /**
     * 大区总的 岗位
     */
    public List<String> regionalManager;

    /**
     * 财务部的 岗位
     */
    public List<String> finance;

    /**
     * 分部经理岗位编码
     */
    public List<String> branchManager;

    /**
     * 查看所有订单的岗位
     */
    public List<String> lookAllOrderPost;


    public List<String> getLookAllOrderPost() {
        return lookAllOrderPost;
    }

    public void setLookAllOrderPost(List<String> lookAllOrderPost) {
        this.lookAllOrderPost = lookAllOrderPost;
    }

    public List<String> getRegionalManager() {
        return regionalManager;
    }

    public void setRegionalManager(List<String> regionalManager) {
        this.regionalManager = regionalManager;
    }

    public List<String> getBranchManager() {
        return branchManager;
    }

    public void setBranchManager(List<String> branchManager) {
        this.branchManager = branchManager;
    }

    public List<String> getFinance() {
        return finance;
    }

    public void setFinance(List<String> finance) {
        this.finance = finance;
    }
}

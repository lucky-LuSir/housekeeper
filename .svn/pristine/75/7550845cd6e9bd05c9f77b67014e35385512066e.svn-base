        </if>
    </select>

    <!-- 通过code批量删除记录 -->
    <delete id="deletePercentageByOrderCode" parameterType="map">
        DELETE FROM t_hkp_trade_order_percentage WHERE order_code = #{orderCode}
        <if test="notIds != null">
            <choose>
                <when test="isPage != null">
                    AND id NOT IN (${notInIds})
                </when>
                <otherwise>
                    AND id NOT IN
                    <foreach collection="notIds" item="i" open="(" separator="," close=")">
                        #{i}
                    </foreach>
                </otherwise>
            </choose>
        </if>

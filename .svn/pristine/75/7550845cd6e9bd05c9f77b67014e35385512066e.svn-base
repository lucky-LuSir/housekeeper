          from
            t_hkp_trade_order_percentage p
        )TP
        <include refid="selectSql"></include>
    </select>


    <!-- 查询符合条件的实体对象 -->
    <select id="selectByMapAndStatus" parameterType="map"
            resultType="java.math.BigDecimal">
        SELECT sum(tp.profit) from (
            SELECT
              p.profit,p.pe_dept_code,p.pe_emp_code,p.order_time,p.is_deleted,o.order_status
            FROM
              t_hkp_trade_order_percentage p
            INNER JOIN t_hkp_trade_order o on o.order_code = p.order_code
        )TP
        <include refid="selectSql"></include>
        <if test="orderBy != null and orderBy != ''">
            ORDER BY ${orderBy}
        </if>
        <if test="order != null and order != ''">

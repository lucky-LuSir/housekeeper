package com.kfwy.hkp.controller.sys.area.vo;

import com.gniuu.framework.service.AbstractServiceRequest;
import com.kfwy.hkp.sys.area.entity.CompanyAreaEntity;

/**
 * @version 1.0
 * @description TODO
